# --- OPENCLAW GATEWAY ---
# Token de acesso ao gateway (obrigatorio para acesso remoto), voce pode gerar,
# openssl rand -hex 32
OPENCLAW_GATEWAY_TOKEN=seu_token_gateway_aqui

# Senha do gateway (alternativa ao token)
OPENCLAW_GATEWAY_PASSWORD=sua_senha_gateway_aqui

# --- CORE API ACCESS ---
ANTHROPIC_API_KEY=sua_chave_claude
GEMINI_API_KEY=sua_chave_gemini
ZAI_API_KEY=sua_chave_z_ai

# --- CHANNELS ---
TELEGRAM_ENABLED=true
TELEGRAM_TOKEN=seu_token_aqui
TELEGRAM_ALLOWED_USERS=seu_id_usuario

DISCORD_ENABLED=true
DISCORD_TOKEN=seu_token_discord

# --- WEB SKILLS ---
SEARCH_ENGINE_API=google_ou_tavily
SEARCH_API_KEY=sua_chave_busca

# --- PROD CONFIG ---
DEBUG=false
LOG_LEVEL=info
